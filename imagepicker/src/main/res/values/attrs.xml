<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CropImageView">
        <attr name="ipCropMaskColor" format="color"/>
        <attr name="ipCropBorderColor" format="color"/>
        <attr name="ipCropBorderWidth" format="dimension"/>
        <attr name="ipCropFocusWidth" format="dimension"/>
        <attr name="ipCropFocusHeight" format="dimension"/>
        <attr name="ipCropStyle" format="enum">
            <enum name="rectangle" value="0"/>
            <enum name="circle" value="1"/>
        </attr>
    </declare-styleable>
</resources>